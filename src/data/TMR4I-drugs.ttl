:ActAdministerAspirin   :involves   drug:DB00945 .

:ActAdministerClopidogrel   :involves   drug:DB00758 .

:ActAdministerDipyridamole  :involves   drug:DB00975 .

# Thiazide is actually a *category* of drugs! Need to make sure our SPARQL query can deal with that.
:ThiazideAdministeringActs  a   owl:Class;
    owl:equivalentClass     [   a   owl:Restriction;
                                owl:onProperty  :involves;
                                owl:someValuesFrom  [
                                    a   owl:Restriction ;
                                    owl:onProperty  drugbank:drugCategory ;
                                    owl:hasValue    <http://www4.wiwiss.fu-berlin.de/drugbank/resource/drugcategory/diuretics,Thiazide> 
                                ] 
                            ] ;
    owl:oneOf       ( :ActAdministerThiazide ) .

:ActAdministerThiazide  :involves <http://www4.wiwiss.fu-berlin.de/drugbank/resource/drugcategory/diuretics,Thiazide> .

:ActAdministerTramadol  :involves drug:DB00193 .

# PPI = Proton Pump Inhibitor... again, this is a *category* of drugs.
:ActAdministerPPI   :involves <http://www4.wiwiss.fu-berlin.de/drugbank/resource/drugcategory/proton-pumpInhibitors> .


:ActAdministerIbuprofen :involves   drug:DB01050 .
